generator client {
    provider = "prisma-client-js"
    output = "../../../node_modules/@prisma-client/jobber"
}

datasource db {
    provider = "postgresql"
    url = env("DATABASE_URL")
}

model User {
    id        String   @id @default(cuid())
    email     String   @unique
    password  String
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}